<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <IncludeBuildOutput>false</IncludeBuildOutput>
  </PropertyGroup>

  <!--Include the assembly inside the interactive-extensions/dotnet/cs folder-->
  <ItemGroup>
    <None Include="$(OutputPath)\$(AssemblyName).dll" Pack="true" PackagePath="interactive-extensions/dotnet/cs" Visible="false" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.DotNet.Interactive" Version="1.0.0-beta.19468.1" />
    <PackageReference Include="Microsoft.DotNet.Interactive.Rendering" Version="1.0.0-beta.19468.1" />
    <PackageReference Include="system.reactive" Version="4.1.6" />
  </ItemGroup>

</Project>
