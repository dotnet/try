{
    "compilerOptions": {
        "allowJs": true,
        "experimentalDecorators": true,
        "jsx": "react",
        "lib": [
            "es6",
            "dom"
        ],
        "module": "commonjs",
        "noImplicitAny": true,
        "outDir": "./obj",
        "sourceMap": true,
        "target": "es6",
        "pretty": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
    },
    "exclude": [
        "node_modules"
    ]
}
