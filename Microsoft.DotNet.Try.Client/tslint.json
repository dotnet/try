{
	"rulesDirectory": [
		"tslint-no-unused-expression-chai"
	],
	"rules": {
		"align": [
			true
		],
		"array-type": [
			true,
			"array-simple"
		],
		"await-promise": true,
		"class-name": true,
		"curly": true,
		"eofline": true,
		"member-access": true,
		"no-return-await": true,
		"no-unused-expression-chai": true,
		"no-duplicate-variable": true,
		"promise-function-async": true,
		"semicolon": true,
		"triple-equals": true,
		"quotemark": true
	}
}