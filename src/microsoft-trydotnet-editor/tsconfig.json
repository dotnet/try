{
	"compilerOptions": {
		"sourceMap": true,
		"module": "commonjs",
		"moduleResolution": "node",
		"target": "ES6",
		"outDir": "./lib",
		"lib": [
			"dom",
			"es2021"
		],
		"types": [],
		"baseUrl": "./node_modules",
		"jsx": "preserve",
		"typeRoots": [
			"node_modules/@types"
		]
	},
	"include": [
		"./src/**/*"
	],
	"exclude": [
		"node_modules"
	]
}